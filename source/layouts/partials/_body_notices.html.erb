<div id="notices">
<% if data.content.notices.count > 0 %>
  <% data.content.notices.each do |n_id, n| %>
  <div class="notice" data-type="<%= n.notice_type || 'info' %>" id="<%= n.slug %>">
    <h2><%= n.notice_heading %></h2>
    <div><%= n.notice_message %></div>
  </div>
  <% end %>
<% end %>
</div>
