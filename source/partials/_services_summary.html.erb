<dl>
  <dt>title</dt>
  <dd><%= service.title %></dd>
  <dt>slug</dt>
  <dd><%= service.slug %></dd>
  <dt>description</dt>
  <dd><%= Kramdown::Document.new(service.description).to_html %></dd>
  <dt>photo</dt>
  <dd><%= "<img src=\"#{service.photo.url}?w=400\" alt=\"#{service.title}\">" if service.photo %></dd>
</dl>
